<%- include('./partials/header.ejs')%>
    <div class="mt-3 container-fluid" style="width: 100%;">
        <h1>Data</h1>
        <form action="" method="get">
            <div class="form-floating">
                <input type="text" class="form-control" name="string" id="string">
                <label for="string">Search</label>
            </div>
        </form>
        <div class="row" style="width: 100%;">
            <% data.forEach((item,i)=>{%>
                <% if(i % 3===0){%>
                    <div class="w-100 mt-4"></div>
                    <%}%>
                        <div class="col mx-3 card" style="width: 10%;">
                            <div class="card-body">
                                DATA <%=item.integer%>
                            </div>
                            <div class="card-footer">
                                <button class="btn btn-warning text-light">
                                    <%=item.string%>
                                </button>
                            </div>
                        </div>
                        <%})%>
        </div>
    </div>
    <%- include('./partials/footer.ejs')%>